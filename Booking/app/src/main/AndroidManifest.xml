<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.booking">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Booking"
        android:usesCleartextTraffic="true">
        <activity android:name=".ServiceActivivty"></activity>
        <activity android:name=".RegisterActivity" />
        <activity android:name=".CartActivity" />
        <activity android:name=".YourbookingdetailActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".MapsActivity"/>
        <activity android:name=".EditProfileActivity" />
        <activity android:name=".YourBookingActivity" />
        <activity android:name=".BookingActivity" />
        <activity android:name=".RoomDetailActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".ChartActivity" />
        <activity android:name=".BarChartActivity" />
        <activity android:name=".PieChartActivity" />
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>